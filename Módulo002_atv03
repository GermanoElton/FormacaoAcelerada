#incluide <stdio.h>
#incluide <stdlib.h>

int main (void) {

    p = (int *) calloc(5, sizeof(int));
    início = p

    for (i = 0; i < 5; i++, p++) {
        printf("%d\n", *p);
    }

    printf("\n");

    p = início;
    p = (int *) realloc(p, 22*sizeof(int));

    for (i = 0; i < 22; i++; p++) {
        scanf("%d", p);
    } 
 
    printf("\n");

    p = início;

    for (i = 0; i < 22; i++; p++) {
        printf("%d", *p);
    }

    free(p);

    return 0;

}
